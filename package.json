{
  "name": "photo-tools",
  "version": "0.0.2",
  "private": true,
  "description": "a set of photo organizing scripts and tools, based on nodejs",
  "type": "module",
  "scripts": {
    "clean": "rimraf node_modules/",
    "devrun": "clear && ./devrun",
    "devrun:loop": "saveloop . clear ./devrun",
    "lint": "eslint src test",
    "lint:fixall": "./script/lint-all.js",
    "lint:fixall2": "eslint src/**/*.js test/**/*.js --fix",
    "syncTestdata": "./sync-testfiles",
    "relink": "npm link photo-testfiles && npm link @nocke/util && ls -lar `npm config get prefix`/lib/node_modules `npm config get prefix`/lib/node_modules/@nocke",
    "test": "mocha --loader=esmock",
    "test-util:loop": "saveloop /depot/PUBLIC/@nocke/util/ clear 'npm t'",
    "test:loop": "saveloop . clear \"npm t $npm_config_test\""
  },
  "dependencies": {
    "@nocke/util": "^0.0.2",
    "commander": "^9.4.1",
    "enquirer": "^2.3.6",
    "trash": "^8.1.1"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.32.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "esmock": "^2.1.0",
    "jest": "^29.4.1",
    "mocha": "^10.2.0",
    "photo-testfiles": "github:nocke/photo-testfiles#master",
    "rimraf": "^4.1.2",
    "sinon": "^15.0.1"
  }
}
